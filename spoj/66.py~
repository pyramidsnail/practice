test = int(raw_input())
girl = [int(x) for x in raw_input().lstrip('0').split()]
line=raw_input()
while line!='0':
    boy = [int(x) for x in line.lstrip('0').split()]
    dp=[[0 for x in xrange(2100)] for x in xrange(2100) ]
    for i in xrange(len(girl)):
        for j in xrange(i,len(boy)):
            if boy[j]==girl[i]:
                dp[i][j] = dp[i-1][j-1]+1


    line=raw_input()
